<defaults>
	<bridge_vlan>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<vlanid>14500</vlanid>
			<name/>
			<active>1</active>
			<ismgmt>0</ismgmt>
			<ifaces></ifaces>
		</settings>
	</bridge_vlan>
	<static>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<ip>0.0.0.0</ip>
			<mask>255.255.255.0</mask>
			<gw>0.0.0.0</gw>
			<dns1>0.0.0.0</dns1>
			<dns2>0.0.0.0</dns2>
			<mode>0</mode>
		</settings>
	</static>
	<dhcps>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<name>My DHCP Server</name>
			<iface>lan-1</iface>
			<enabled>1</enabled>
			<startip>192.168.3.2</startip>
			<endip>192.168.3.254</endip>
			<subnet>255.255.255.0</subnet>
			<gw>192.168.3.1</gw>
			<dns>192.168.3.1</dns>
			<leasetime>3600</leasetime>
			<options/>
			<clients/>
                        <auto_opmode>
                                <enabled>1</enabled>
                                <detection_period>10</detection_period>
                                <timeout_factor>3</timeout_factor>
                        </auto_opmode>
		</settings>
	</dhcps>
	<dhcprelay>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<name/>
			<state/>
			<connection_lan/>
			<connection_wan/>
			<server_ip/>
		</settings>
	</dhcprelay>
	<lan>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<type>none</type>
			<entities>
				<static></static>
				<bridge></bridge>
			</entities>
		</settings>
	</lan>
	<rfc2684>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<encaps>LLC</encaps>
			<vlanid>0</vlanid>
		</settings>
	</rfc2684>
	<pvc>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<name>vc2</name>
			<vpi>8</vpi>
			<vci>36</vci>
			<traffic_class>UBR</traffic_class>
			<pcr>0</pcr>
			<scr>0</scr>
			<mbs>0</mbs>
	    </settings>
	</pvc>
	<pppox>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<type>pppoe</type>
			<username>kullanici_adiniz</username>
			<password>test</password>
			<mtu>1492</mtu>
			<lcp_echo_failure>10</lcp_echo_failure>
			<lcp_echo_interval>60</lcp_echo_interval>
			<ondemand>0</ondemand>
			<idle>600</idle>
			<service_name/>
			<ac_name/>
		</settings>
	</pppox>
	<wan>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<enabled>0</enabled>
			<l2type></l2type>
			<linktype></linktype>
			<l2device></l2device>
			<entities/>
		</settings>
	</wan>
	<dhcpc>
		<log>
			<level>crit</level>
		</log>
		<settings/>
	</dhcpc>
	<port_vlan>
		<port type="wds">
			<iface>wds-0</iface>
			<name>WDS 1</name>
			<pvid>0</pvid>
			<tagging>0</tagging>
			<portpriority>0</portpriority>
			<funtag>0</funtag>
			<funreg>0</funreg>
		</port>
		<port type="eth">
			<iface>eth-0</iface>
			<name>Ethernet 1</name>
			<pvid>0</pvid>
			<tagging>0</tagging>
			<portpriority>0</portpriority>
			<funtag>0</funtag>
			<funreg>0</funreg>
		</port>
		<port type="wireless">
			<iface>wireless-0</iface>
			<name>Wireless 1</name>
			<pvid>0</pvid>
			<tagging>0</tagging>
			<portpriority>0</portpriority>
			<funtag>0</funtag>
			<funreg>0</funreg>
		</port>
	</port_vlan>
	<wireless_wds>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<enabled>0</enabled>
			<mac></mac>
			<security>
				<mode>off</mode>
				<wep>
					<auth_type></auth_type>
					<key_type></key_type>
					<key_index></key_index>
					<key1></key1>
					<key2></key2>
					<key3></key3>
					<key4></key4>
				</wep>
			</security>
		</settings>
	</wireless_wds>
	<wan_vlan>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<name/>
			<vlanid/>
			<port/>
		</settings>
	</wan_vlan>
	<usb>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<name/>
			<state/>
		</settings>
	</usb>
	<vpn>
		<log>
			<level>crit</level>
		</log>
		<settings>
			<enabled>0</enabled>
			<type>pptp</type>
			<server>defaultserver.com</server>
			<users>
				<user>
					<username>default_user</username>
					<password>default_password</password>
				</user>
			</users>
			<options>
				<option id="usepeerdns">1</option>
				<option id="refuse-pap">0</option>
				<option id="refuse-eap">0</option>
				<option id="require-mppe-128">0</option>
			</options>
			<defaultroute>0</defaultroute>
			<dnsprio>5</dnsprio>
			<mtu></mtu>
		</settings>
	</vpn>
</defaults>
